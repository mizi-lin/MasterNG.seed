{
    "name": "angular2-webpack-seed",
    "version": "0.0.0",
    "description": "Angular2 Webpack Seed",
    "homepage": "https://github.com/r-park/angular2-webpack-seed",
    "repository": {
        "type": "git",
        "url": "https://github.com/r-park/angular2-webpack-seed.git"
    },
    "author": {
        "name": "Richard Park",
        "email": "objectiv@gmail.com"
    },
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=5.10"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "clean": "del-cli ./target",
        "lint": "npm-run-all lint:js lint:ts",
        "lint:js": "eslint -c .eslintrc.yml *.js ./src ./server",
        "lint:ts": "tslint './src/**/*.ts'",
        "postinstall": "npm run typings",
        "prebuild": "npm-run-all clean lint:ts",
        "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
        "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
        "server:test": "cross-env NODE_ENV=test webpack-dev-server --progress",
        "server:prod": "cross-env NODE_ENV=production webpack-dev-server --progress",
        "test": "npm run server:test",
        "start": "npm run server:dev",
        "test:watch": "cross-env NODE_ENV=test karma start",
        "typings": "typings install",
        "version": "npm run changelog && git add CHANGELOG.md"
    },
    "dependencies": {
        "@angular/common": "2.0.0-rc.4",
        "@angular/compiler": "2.0.0-rc.4",
        "@angular/core": "2.0.0-rc.4",
        "@angular/forms": "0.1.1",
        "@angular/http": "2.0.0-rc.4",
        "@angular/platform-browser": "2.0.0-rc.4",
        "@angular/platform-browser-dynamic": "2.0.0-rc.4",
        "@angular/router": "3.0.0-beta.2",
        "core-js": "^2.4.0",
        "rxjs": "5.0.0-beta.6",
        "zone.js": "^0.6.12"
    },
    "devDependencies": {
        "autoprefixer": "~6.3.6",
        "babel-loader": "^6.2.5",
        "conventional-changelog-cli": "~1.2.0",
        "copy-webpack-plugin": "~3.0.1",
        "cross-env": "~1.0.8",
        "css-loader": "~0.23.1",
        "del-cli": "~0.2.0",
        "eslint": "~2.13.1",
        "express": "~4.14.0",
        "extract-text-webpack-plugin": "~1.0.1",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "~2.21.0",
        "jasmine-core": "~2.4.1",
        "karma": "~1.1.0",
        "karma-chrome-launcher": "~1.0.1",
        "karma-jasmine": "~1.0.2",
        "karma-sourcemap-loader": "~0.3.7",
        "karma-webpack": "~1.7.0",
        "morgan": "~1.7.0",
        "mujs": "^1.0.1",
        "node-sass": "~3.8.0",
        "nodemon": "~1.9.2",
        "npm-run-all": "~2.3.0",
        "postcss-loader": "~0.9.1",
        "raw-loader": "~0.5.1",
        "sass-loader": "~4.0.0",
        "style-loader": "~0.13.1",
        "ts-loader": "~0.8.2",
        "tslint": "~3.12.1",
        "typescript": "~1.8.10",
        "typings": "~1.3.1",
        "url-loader": "^0.5.7",
        "web-animations-js": "^2.2.2",
        "webpack": "~1.13.1",
        "webpack-dev-server": "~1.14.1",
        "webpack-md5-hash": "~0.0.5",
        "winston": "~2.2.0"
    }
}
